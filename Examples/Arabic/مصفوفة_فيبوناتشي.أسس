اشمل "مـتم/طـرفية.أسس"؛

عرف مـصفوفة_فـيبوناتشي: حزمة
{
  عرف إبدأ: دالة ()
  {
    عرف ع: صـحيح؛

    // أنشئ المصفوفة.
    عرف م: مصفوفة[صـحيح، 10]؛
    م(0) = 1؛
    م(1) = 1؛
    لكل ع = 2، ع < 10، ++ع {
      م(ع) = م(ع-1) + م(ع-2)
    }؛

    // اطبع المصفوفة.
    لكل ع = 0، ع < 10، ++ع {
      مـتم.طـرفية.اطبع("%d\ج"، م(ع))
    }
  }
}؛

نفذ مـصفوفة_فـيبوناتشي.إبدأ

