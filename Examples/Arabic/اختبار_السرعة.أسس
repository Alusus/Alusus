اشمل "مـتم/طـرفية.أسس"؛

حزمة اختبار_السرعة {
    عرف ساعة: @تصدير[clock] دالة (): صـحيح[64]؛
    دالة البدايه {
        عرف الرقم: صـحيح[64] = 6673214311؛
        عرف بادية_الوقت: صـحيح[64] = ساعة()؛
        مـتم.طـرفية.اطبع("%ld هو ", الرقم)؛
        إذا (هل_هو_اولي(الرقم)){
            مـتم.طـرفية.اطبع("اولي\n")؛
        } وإلا {
            مـتم.طـرفية.اطبع("ليس اولي\n")؛
        }؛

        عرف نهاية_الوقت: صـحيح[64] = ساعة()؛
        عرف وقت_التنفيذ: صـحيح[64] = (نهاية_الوقت - بادية_الوقت)؛
        مـتم.طـرفية.اطبع("العملية اخذت %d ثانية\n", (وقت_التنفيذ/1000000))؛
    }؛

    دالة هل_هو_اولي(الرقم: صـحيح[64]): ثنائي {
        عرف اولي: ثنائي = 1؛
        عرف ف: صـحيح[64]؛
        لكل ف=2, ف<= (الرقم/2), ف++ {
            إذا ((الرقم % ف) == 0)  {
                اولي = 0؛
                اقطع؛
            }
        }؛

        أرجع اولي؛
    }؛
}؛

اختبار_السرعة.البدايه()؛
