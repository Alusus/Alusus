{
  عرف إبدأ: دالة ()
  {
    عرف س: ـصحيح[32]؛
    عرف ص: ـعائم[64]؛
    س = 34ص16؛
    ص = 5.12ع32؛
    ـمتم.اطبع("س = %d، ص = %f\ج"، س، ص)؛
    س = 0ن100؛
    ـمتم.اطبع("س = %d، ص = %f\ج"، س، ص)؛
    س = 0م11؛
    ـمتم.اطبع("س = %d، ص = %f\ج"، س، ص)؛
  }
}؛

