import "libalusus_spp.so";

def Main: module
{
  def print: @expname[printf] function (fmt: ptr[Word[8]], args: ...any)=>Int[64];

  def start: function () => Void
  {
    def i: Int;
    def j: Int;

    i = 0;
    while i < 10 {
      j = 0;
      while j <= i {
        print("%d", j);
        ++j;
      };
      print("\n");
      ++i;
    };

    for i = 0, i < 10, ++i {
      for j = 0, j <= i, ++j {
        print("%d", j);
      };
      print("\n");
    };
  };
};

run Main.start;
