import "Srl/Console.alusus";

def TemperatureTable1: module
{
  def start: function ()
  {
    def fahr : Int;
    def celsius : Int;
    def lower : Int;
    def upper : Int;
    def step : Int;
    lower = 0;
    upper = 300;
    step = 20;

    fahr = lower;
    while fahr <= upper {
      celsius = 5 * (fahr - 32) / 9;
      Srl.Console.print("%d\t%d\n", fahr, celsius);
      fahr = fahr + step;
    }
  }
};

run TemperatureTable1.start;
