import "common.alusus";

def TemperatureTable1: module {
  def start: function () {
    def fahr : Int[32];
    def celsius : Int[32];
    def lower : Int[32];
    def upper : Int[32];
    def step : Int[32];
    lower = 0;
    upper = 300;
    step = 20;

    fahr = lower;
    while fahr <= upper {
      celsius = 5 * (fahr - 32) / 9;
      Srt.print("%d\t%d\n", fahr, celsius);
      fahr = fahr + step;
    }
  }
};

run TemperatureTable1.start;
