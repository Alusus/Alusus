import "srt.alusus";

def FactorialWhileLoop: module
{
  def factorial: function (i:Int) => Int
  {
    if i<=0 return 1;
    def f: Int;
    f = i;
    while i >= 2 {
      --i;
      f = f*i;
    };
    return f;
  };

  def start: function()
  {
    Srt.Console.print("Factorial of 5 is %d\n", factorial(5));
  }
};

run FactorialWhileLoop.start;
