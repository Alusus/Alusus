import "common.alusus";

def FactorialWhileLoop: module {
  def factorial: function (i:Int[32]) => Int[32] {
    if i<=0 return 1;
    def f: Int[32];
    f = i;
    while i >= 2 {
      --i;
      f = f*i;
    };
    return f;
  };

  def start: function() {
    Srt.print("Factorial of 5 is %d\n", factorial(5));
  }
};

run FactorialWhileLoop.start;
