import "common.alusus";

def HelloWorld : module {
  def openTestFile : function() => ptr[int] {
    return fopen("/tmp/AlususEndToEndTest.txt", "w+");
  };

  def main : function() => int {
    def f : ptr[int];
    f := openTestFile();
    writeToFile(f, "Hello World!\n");
    closeFile(f);
    return 0;
  };
};

run HelloWorld;
