import "common.alusus";

def Program1_2_1 : module {
  def main : function() {
    def fahr : int;
    def celsius : int;
    def lower : int;
    def upper : int;
    def step : int;
    lower := 0;
    upper := 300;
    step := 20;
    
    fahr := lower;
    while (fahr <= upper) {
      celsius := 5 * (fahr - 32) / 9;
      printf("%d\t%d\n", fahr, celsius);
      fahr := fahr + step;
    }
  }
};

run Program1_2_1;