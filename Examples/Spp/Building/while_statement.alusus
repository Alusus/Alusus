import "libalusus_spp.so";

def Main: module
{
  def print: @expname[printf] function (fmt: ptr[Int[8]], args: ...any)=>Int[64];

  def start: function () => Void
  {
    def i: Int[32];
    i = 10;

    while i > 0 print("while-statement: %d\n", i);

    while i > 0 {
      print("while-block: %d\n", i);
      i = i - 1
    };
  };
};

build Main;
