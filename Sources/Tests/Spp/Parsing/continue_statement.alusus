import "libalusus_spp";

def Main: module
{
  def start: function ()=>Void
  {
    def i: Int[32];

    while i > 0 {
      continue;
      continue 2;
      continue "abc";
      continue abc;
    };

    for i = 0, i < 5, ++i {
      continue;
      continue 2;
      continue "abc";
      continue abc;
    }
  };
};

dump_ast Main;
