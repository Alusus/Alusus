import "alusus_spp";

def main: function ()=>Void
{
  def i: Int[32];
  def pi: ptr[Int[32]];
  def a: array[Int[32], 5];
  def pa: ptr[array[Int[32], 5]];

  //
  // ~ptr & ~cnt
  //

  i~ptr;
  i~ptr~cnt;

  pi = i~ptr;
  pi~cnt = 5;

  i = pi~cnt + 3;

  a~ptr~cnt(3) = 3;

  pa = a~ptr;
  pa~cnt(3) = 4;

  i~p;
  i~ptr a;

  //
  // ~cast
  //

  pi~cast[ptr[Int[16]]] = 5;
  i~ptr~cast[ptr[Int[16]]]~cnt = 5;
  pa~cast(ptr[Int[16]]) = 5;
  pa~cast ptr[Int[16]] = 10;

  //
  // ~size
  //

  i~size 5;

  i = pi~size;
  i = 5~size;
  i = 5 + i~size;
  i = (i + 5i64)~size;
  i = Int[32]~size;
};

dump_ast main;
