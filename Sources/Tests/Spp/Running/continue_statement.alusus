import "libalusus_spp.so";

def Main: module
{
  def print: @expname[printf] function (fmt: ptr[Word[8]], args: ...any)=>Int[64];

  def start: function () => Void
  {
    def i: Int[32];
    def j: Int[32];

    i = 0;
    while i < 10 {
      j = 0;
      while j <= i {
        if i == 5 {
          if j == 2 {
            ++j;
            continue;
          };
          if j == 4 {
            ++j;
            continue 1;
          }
        };
        if i == 8 {
          if j == 4 {
            ++i;
            continue 2;
          };
        };
        print("%d", j);
        ++j;
      };
      print("\n");
      ++i;
    };

    for i = 0, i < 10, ++i {
      for j = 0, j <= i, ++j {
        if i == 5 {
          if j == 2 continue;
          if j == 4 continue 1;
        };
        if i == 8 {
          if j == 4 continue 2;
        };
        print("%d", j);
      };
      print("\n");
    };
  };
};

run Main.start;
