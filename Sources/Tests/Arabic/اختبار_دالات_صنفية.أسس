اشمل "مـتم/طـرفية.أسس"؛

صنف الـف
{
  عرف م: صحيح؛
  
  @عضو دالة اطبع (هذا: سند[هذا_الصنف])
  {
    مـتم.طـرفية.اطبع("هذا.م = %d\ج"، هذا.م)؛
  }؛
  
  @عضو دالة printDouble (هذا: سند[هذا_الصنف])
  {
    مـتم.طـرفية.اطبع("هذا.م * 2 = %d\ج"، هذا.م * 2)؛
  }؛
  
  عرف اطبع_مضاعف: لقب printDouble؛
}؛

عرف ا: الـف؛
ا.م = 3؛
ا.اطبع()؛
ا.اطبع_مضاعف()؛

