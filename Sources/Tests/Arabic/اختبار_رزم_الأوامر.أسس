اشمل "مـتم/طـرفية"؛

مـتم.طـرفية.اطبع("مضروب %d=%d\ج"، 5، صحيح()~استخدم_في(أنا){ عرف ن: صحيح(5)؛ أنا = ن؛ بينما --ن > 1 أنا *= ن })؛

صنف صـنفي {
    عرف ع: صحيح؛
}

مـتم.طـرفية.اطبع("صـنفي.ع = %d\ج"، صـنفي()~استخدم_في(أنا){ ع = 3 }.ع)؛

مـتم.طـرفية.اطبع("صـنفي.ع = %d\ج"، صـنفي()~استخدم_في(أنا) بلا_حقن { أنا.ع = 4 }.ع)؛

مـتم.طـرفية.اطبع("صـنفي.ع = %d\ج"، صـنفي()~استخدم_في(أنا) بلا_حقن { ع = 5 }.ع)؛

