import "libalusus_scg.so";

def FunctionOverloading : module {
  link {
    printf(string, args) => int;
  };
  
  def printValue : function(value : int) {
    printf("Printing integral value: %d\n", value);
  };
  
  def printValue : function(value : double) {
    printf("Printing double value: %f\n", value);
  };
  
  def main : function() {
    printValue(5);
    printValue(5.5);
    return 0;
  }
};

run FunctionOverloading;
