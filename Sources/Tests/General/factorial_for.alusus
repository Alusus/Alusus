import "common.alusus";

def FactorialForLoop : module {
  def factorial : function(i:Int) => Int {
    if i<=0 return 1;
    def f : Int;
    f = i;
    for i=i-1, i>=2, i=i-1 f=f*i;
    return f;
  };

  def main : function() {
    printf("Factorial of 5 is %d\n", factorial(5));
    return 0 
  };
};

run FactorialForLoop;
