اشمل "مـتم/طـرفية"؛
اشمل "مغلفة"؛

استخدم مـتم.طـرفية؛

دالة نفذ (م: مغلفة(صحيح):صحيح) {
    عرف ر: صحيح = م(5)؛
    اطبع("نفذ: %d\ج"، ر)؛
}

دالة جهز (): (مغلفة(صحيح):صحيح) {
    عرف ع: صحيح = 6؛
    أرجع مغلفة (ت: صحيح): صحيح {
        اطبع("جهز: %d %d\ج"، ت، ع)؛
        أرجع ع؛
    }؛
}

دالة اختبر {
    عرف م: مغلفة(صحيح):صحيح = جهز()؛
    نفذ(م)؛
}
اختبر()؛

