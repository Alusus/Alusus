اشمل "ـمتم/ـنظام.أسس"؛
اشمل "ـمتم/ـطرفية.أسس"؛

عرف ـنظام: حزمة
{
  عرف ابدأ: دالة ()
  {
    // نم & هات_الختم_الزمني
    عرف خز: ـطبيعي[64]؛
    خز = ـمتم.ـنظام.هات_الختم_الزمني(0)؛
    ـمتم.ـنظام.نم(1000000)؛
    خز = ـمتم.ـنظام.هات_الختم_الزمني(0) - خز؛
    ـمتم.ـطرفية.اطبع("%llu\ج"، خز)؛

    // شغل
    ـمتم.ـنظام.شغل("LANG=en_US date -d @0")؛
    
    // هات_الوقت_المفصل
    عرف وقت: ـمتم.ـنظام.ـوقت؛
    عرف مؤشر_وقت: مؤشر[ـمتم.ـنظام.ـوقت]؛
    خز = 1535881398؛
    ـمتم.ـنظام.هات_الوقت_المفصل(خز~مؤشر، وقت~مؤشر)؛
    ـمتم.ـطرفية.اطبع("%d-%02d-%02d\ج"، وقت.سنة + 1900، وقت.شهر + 1، وقت.يوم)؛
    خز = 0؛
    مؤشر_وقت = ـمتم.ـنظام.هات_الوقت_المفصل(خز~مؤشر)؛
    ـمتم.ـطرفية.اطبع("%d-%02d-%02d\ج"، مؤشر_وقت~محتوى.سنة + 1900، مؤشر_وقت~محتوى.شهر + 1، مؤشر_وقت~محتوى.يوم)؛
  };
};

نفذ ـنظام.ابدأ؛

