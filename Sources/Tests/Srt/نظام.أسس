اشمل "مـتم/نـظام.أسس"؛
اشمل "مـتم/طـرفية.أسس"؛

عرف نـظام: وحدة
{
  عرف ابدأ: دالة ()
  {
    // نم & هات_الختم_الزمني
    عرف خز: طـبيعي[64]؛
    خز = مـتم.نـظام.هات_الختم_الزمني(0)؛
    مـتم.نـظام.نم(1000000)؛
    خز = مـتم.نـظام.هات_الختم_الزمني(0) - خز؛
    مـتم.طـرفية.اطبع("%llu\ج"، خز)؛

    // شغل
    مـتم.نـظام.شغل("TZ=UTC LANG=en_US date -d @0")؛

    // اضبط_متغير_محيطي & هات_متغير_محيطي
    مـتم.نـظام.اضبط_متغير_محيطي("TZ"، "UTC"، 1)؛
    مـتم.طـرفية.اطبع(مـتم.نـظام.هات_متغير_محيطي("TZ"))؛
    مـتم.طـرفية.اطبع("\ج")؛

    // هات_الوقت_المفصل
    عرف وقت: مـتم.نـظام.وقـت؛
    عرف مؤشر_وقت: مؤشر[مـتم.نـظام.وقـت]؛
    خز = 1535881398؛
    مـتم.نـظام.هات_الوقت_المفصل(خز~مؤشر، وقت~مؤشر)؛
    مـتم.طـرفية.اطبع("%d-%02d-%02d\ج"، وقت.سنة + 1900، وقت.شهر + 1، وقت.يوم)؛
    خز = 0؛
    مؤشر_وقت = مـتم.نـظام.هات_الوقت_المفصل(خز~مؤشر)؛
    مـتم.طـرفية.اطبع("%d-%02d-%02d\ج"، مؤشر_وقت~محتوى.سنة + 1900، مؤشر_وقت~محتوى.شهر + 1، مؤشر_وقت~محتوى.يوم)؛
  };
};

نـظام.ابدأ()؛
