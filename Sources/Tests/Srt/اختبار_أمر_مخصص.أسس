اشمل "الـقلب/أسـاسيات"؛
اشمل "نـبم"؛
اشمل "مـتم/سندات"؛
اشمل "مـتم/تـطبيق"؛
اشمل "مـتم/نـص"؛

تمهيد {
    استخدم مـتم؛
    عرف سـندنا_بهوية: لقب سـندنا[الـقلب.أسـاسيات.كـائن_بهوية]؛
    نـبم.مدير_القواعد.أضف_أمرا_مخصصا(
        "أمـر_اختبار"،
        (شبم {
            مفاتيح: "أمر_اختبار"؛
            معطيات: "module".Expression*(1,1)؛
        })،
        دالة (معطيات: سـندنا_بهوية): سـندنا_بهوية {
            عرف حاوية: الـقلب.أسـاسيات.حـاوية[معطيات.كائن]؛
            أرجع نـبم.مدير_شبم.أنشئ_شبم(
                (شبم أمر_اختبار[معطى])،
                تـطبيق[نـص، سند[الـقلب.أسـاسيات.كـائن_بهوية]]().حدد(نـص("معطى")، حاوية.هات_عنصرا(1))
            )؛
        }
    )؛
}؛

عرف س: {
    أمر_اختبار ا + ب؛
}
أدرج_ش_ب_م س؛

