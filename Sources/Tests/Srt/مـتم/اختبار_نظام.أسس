اشمل "مـتم/نـظام.أسس"؛
اشمل "مـتم/طـرفية.أسس"؛
اشمل "مـتم/نـص"؛

عرف نـظام: وحدة
{
  عرف ابدأ: دالة ()
  {
    // نم
    مـتم.نـظام.نم(1000000ط)؛

    // شغل
    إذا مـتم.نـص.أمتطابق(الـعملية.النظام، "linux") {
      مـتم.نـظام.شغل("TZ=UTC LANG=en_US date -d @0")؛
    } وإلا {
      مـتم.نـظام.شغل("TZ=UTC LANG=en_US date -r 0")؛
    }

    // اضبط_متغير_محيطي & هات_متغير_محيطي
    مـتم.نـظام.اضبط_متغير_محيطي("TZ"، "UTC"، 1)؛
    مـتم.طـرفية.اطبع(مـتم.نـظام.هات_متغير_محيطي("TZ"))؛
    مـتم.طـرفية.اطبع("\ج")؛
  };
};

نـظام.ابدأ()؛
