        عرف عرب_صنف_شبم: لقب الـقلب.بـيانات.شـبم.عرب_صنف_شبم؛
        
        عرب_صنف_شبم[Type, صـنف];
        عرب_صنف_شبم[DataType, صـنف_بيانات];
