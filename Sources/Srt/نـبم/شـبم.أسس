        عرب_صنف_شبم[UserType, صـنف_مستخدم];
        عرب_صنف_شبم[FunctionType, صـنف_دالة];

        عرب_صنف_شبم[Template, قـالب];
        عرب_صنف_شبم[TemplateVarDef, تـعريف_معطى_قالب];

