اشمل "Core"؛

عرف الـقلب: لقب Core؛
@دمج عرف Core: وحدة
{
    عرف اشمل_ملف: لقب importFile؛
    عرف هات_من_الخزينة_العامة: لقب getFromGlobalStorage؛

    عرف مـصنع_كائن_جلي: لقب TiObjectFactory؛
    @دمج صنف مـصنع_كائن_جلي {
        عرف أنشئ_محتكر: لقب createPlain؛
        عرف أنشئ_مشترك: لقب createShared؛
        عرف هيئ: لقب initialize؛
        عرف أتلف: لقب terminate؛
    }

    عرف هـوية_صنف: لقب TypeInfo؛
    @دمج صنف هـوية_صنف {
        عرف اسم_الصنف: لقب typeName؛
        عرف مسار_الصنف: لقب typeNamespace؛
        عرف اسم_الحزمة: لقب moduleName؛
        عرف الرابط: لقب url؛
        عرف الاسم_الكامل: لقب uniqueName؛
        عرف هوية_صنف_السلف: لقب baseTypeInfo؛
        عرف مصنع_الكائنات: لقب objectFactory؛
    }

    عرف كـائن_بهوية: لقب TiObject؛
    @دمج صنف كـائن_بهوية {
        عرف هات_هويتي: لقب getMyTypeInfo؛
        عرف أمشتق_من: لقب isDerivedFrom؛
        عرف هات_وسيط: لقب getInterface؛
    }

    عرف وسـيط_بهوية: لقب TiInterface؛

    عرف نـص_بهوية: لقب TiStr؛
    @دمج صنف نـص_بهوية {
        عرف القيمة: لقب value؛
    }

    ماكرو عرب_صنف_شبم_نصيا [الاسم_الانجليزي، الاسم_العربي] {
        عرف الاسم_العربي: لقب الاسم_الانجليزي؛
        @دمج صنف الاسم_العربي {
            عرف المالك: لقب owner؛
            عرف القيمة: لقب value؛
        }
    }

    عرب_صنف_شبم_نصيا[Identifier، مـعرف]؛
    عرب_صنف_شبم_نصيا[StringLiteral، نـص_حرفي]؛
    عرب_صنف_شبم_نصيا[CharLiteral، مـحرف_حرفي]؛
    عرب_صنف_شبم_نصيا[IntegerLiteral، صـحيح_حرفي]؛
    عرب_صنف_شبم_نصيا[FloatLiteral، عـائم_حرفي]؛

    ماكرو عرب_صنف_شبم [الاسم_الانجليزي، الاسم_العربي] {
        عرف الاسم_العربي: لقب الاسم_الانجليزي؛
        @دمج صنف الاسم_العربي {
            عرف المالك: لقب owner؛
        }
    }
}

